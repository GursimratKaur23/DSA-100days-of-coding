#include <stdio.h>

int main()
{
    int arr[100],x,n,pos,i;
    scanf("%d", &n);
    
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    scanf("%d", &x);
    scanf("%d", &pos);
    pos=pos-1;
    for(i=n;i>pos;i--){
        arr[i]=arr[i-1];
    }
    arr[pos]=x;
    n++;
    for(i=0;i<n;i++){
        printf("%d ", arr[i]);
    }
    return 0;
    
}
